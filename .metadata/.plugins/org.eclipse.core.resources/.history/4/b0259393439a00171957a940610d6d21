
import javafx.scene.layout.GridPane;


public class EvaluationGrid {
	
	public static int[][] eval;
	
	public static void evalGrid() {
		double test = HelloWorld.root.getHgap();
		System.out.println(test);
	}
	
	public static void setTable(int x, int y, int val) {
		eval[x][y] = val;
		return;
	}
	
	public static void printTable() {
		for(i = 0; i < eval.length; i++) {
			for(j = 0; j< eval.length; j++) {
				System.out.print(" " + eval[i][j] + " ");
			}
			System.out.println();
		}
	}

}
